{
    "name": "nextjs-gsap",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build --turbopack",
        "start": "next start",
        "lint": "eslint",
        "migrate:dev": "npx dotenv -e .env.development.local -- prisma migrate dev",
        "seed:dev": "npx dotenv -e .env.development.local -- prisma db seed",
        "migrate:prod": "npx dotenv -e .env.production.local -- prisma migrate deploy",
        "seed:prod": "npx dotenv -e .env.production.local -- prisma db seed"
    },
    "prisma": {
        "seed": "tsx prisma/seedData.ts"
    },
    "dependencies": {
        "@apollo/client": "^4.0.4",
        "@gsap/react": "^2.1.2",
        "@tanstack/react-query": "^5.87.4",
        "axios": "^1.11.0",
        "graphql": "^16.11.0",
        "gsap": "^3.13.0",
        "next": "15.5.2",
        "next-auth": "^4.24.11",
        "react": "19.1.0",
        "react-dom": "19.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@prisma/client": "^6.16.1",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20.19.14",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.5.2",
        "prisma": "^6.16.1",
        "tailwindcss": "^4",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
